<% provide(:title, "AEDs") %>

<div class="main-area"><!-- main area -->
  <%= render 'title_bar' %>
  <div class="main-holder"><!-- main holder -->
    <div id="content"><!-- content -->
      <div class="content_right_sidebar"><!-- left content -->
        <%= table_for(@aeds) do |t| %>
            <%= t.head do %>
                <%= t.h('Feature') %>
                <%= t.h('Zoll') %>
                <%= t.h('Philips') %>
                <%= t.h('Physio') %>
            <% end %>
        <% end %>
      </div><!-- end left content -->
      <br class="clear" />
    </div><!-- end content -->
    <div id="sub_nav" class="nav_right_sub_nav"><!-- start sub nav -->
      <%= render 'sub_nav' %>
    </div><!-- end sub nav -->
  </div><!-- end main holder -->
</div><!-- end main area -->
